    <script>
        if (localStorage.search != undefined && localStorage.search.indexOf("partition") != -1 && location.search.indexOf("partition") == -1) {
            if (location.search == "") {
                // location.replace(location.href + localStorage.search); 
                window.history.pushState({}, 0, location.href + localStorage.search);
            } else {
                var search = location.href + "&" + localStorage.search.replace(/\?/, "");
                // location.replace(search); 
                window.history.pushState({}, 0, search);
            }

            // window.history.pushState({},0,location.href+localStorage.search);    
        } 
    </script>
    
    <% if (process.env.NODE_ENV === 'handover') { %>
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <% var cssFile = htmlWebpackPlugin.files.css[css] %>

    <link href="<%= cssFile %>" rel="stylesheet">
    <% } %>

    <% } else { %>
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <% var cssFile = htmlWebpackPlugin.files.css[css].substr(htmlWebpackPlugin.files.publicPath.length); %>

    <link href="<%= cssFile %>" rel="stylesheet">
    <% } %>

    <% } %>

</head>
<body class="">
