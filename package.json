{
    "name": "pixi-h5-boilerplate",
    "version": "0.1.0",
    "description": "基于pixi.js的高性能多动效H5模板",
    "author": "xieshengyong",
    "repository": {
        "type": "git",
        "url": "https://github.com/xieshengyong/pixi-H5-boilerplate"
    },
    "projectConfigs": {
        "_notes_path": "项目路径，dev为开发阶段，一般默认无需配置，dist为相对目录或七牛分离地址，用于html和资源部署在同一入口，prod为分离地址",
        "path": {
            "dev": "",
            "dist": "//test.treedom.cn/jsmpeg/ossweb-img/",
            "prod": "//game.gtimg.cn/images/yxzj/cp/a20210317wwf/"
        },
        "zipName": "a20210104act"
    },
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server --config webpack.config.dev.js",
        "dist": "cross-env NODE_ENV=dist webpack --config webpack.config.prod.js",
        "postdist": "npm run qshell && npm run sftp",
        "replace": "cd dist/ossweb-img/ && replace 'ossweb-img/' '' -r --include=main.*.css,main.css",
        "qn1": "cross-env NODE_ENV2=qn webpack --config webpack.config.handover.js",
        "qn": "cross-env NODE_ENV2=qn webpack --config webpack.config.handover.js",
        "build": "cross-env NODE_ENV=prod webpack --config webpack.config.prod.js",
        "handover1": "webpack --config webpack.config.handover.js && npm run zip",
        "handoverm": "webpack --config webpack.config.handover.js && npm run moveVendor && npm run zip",
        "moveVendor": "move-file dist/a20210317wwf/ossweb-img/vendor.js dist/vendor.js",
        "zip": "cd dist/ && bestzip ./dist.zip *",
        "handover2": "cross-env TEST=true webpack --config webpack.config.handover.js && npm run moveVendor2 && npm run replaceAA && npm run zip2",
        "moveVendor2": "move-file dist/a20210104aa/ossweb-img/vendor.js dist/vendor.js",
        "replaceAA": "cd dist/a20210104aa/ && replace 'a20210317wwf' 'a20210104aa' -r --include=index.html",
        "replace1": "cd dist/ && replace '/ossweb-img/(\\w+).(\\w+).js' '/$1.$2.js' -r --include=index.html",
        "copyfiles": "copyfiles -f dist/ossweb-img/*.js dist",
        "move1": "move-file dist/ossweb-img/main.5031738d.js dist/main.5031738d.js",
        "zip2": "cd dist && bestzip a20210104aa.zip a20210104aa/*",
        "zip3": "bestzip dist/*",
        "sever": "cd dist && browser-sync start -s",
        "qshell": "qshell user cu me && qshell qupload -c 30 .qshell.json",
        "sftp": "node sftp.js",
        "jsmpeg": "ffmpeg -i src/media/video.mp4 -f mpegts -codec:v mpeg1video -s 1600x750 -b:v 800k -r 24 -bf 0 -codec:a mp2 -ar 44100 -ac 1 -b:a 96k src/media/v.ttf"
    },
    "keywords": [
        "h5",
        "animation",
        "pixijs",
        "boilerplate",
        "pixi-h5-boilerplate"
    ],
    "license": "MIT",
